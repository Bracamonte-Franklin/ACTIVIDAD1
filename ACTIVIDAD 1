#include <iostream>

int main() {
    std::cout << "  ##### " << std::endl;
    std::cout << " ##     " << std::endl;
    std::cout << " ##     " << std::endl;
    std::cout << " ##     " << std::endl;
    std::cout << " ##     " << std::endl;
    std::cout << "  ##### " << std::endl;

    // Esperar hasta que el usuario presione Enter para salir
    std::cin.get();

    return 0;
}
